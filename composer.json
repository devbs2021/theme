{
  "name": "devbshrestha/theme",
  "type": "library",
  "description": "Simple Package for admin lte theme",
  "license": "MIT",
  "authors": [
    {
      "name": "Bharat Shrestha",
      "email": "bharatstha07@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "yajra/laravel-datatables-buttons": "^4.0",
    "yajra/laravel-datatables-html": "^4.0",
    "yajra/laravel-datatables-oracle": "~9.0",
    "spatie/laravel-permission": "^4.2",
    "laravel/breeze": "^1.2",
    "spatie/laravel-sluggable": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "DevbShrestha\\Theme\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "DevbShrestha\\Theme\\ThemeServiceProvider"
      ],
      "aliases": {
        "Theme": "DevbShrestha\\Theme\\Facades\\ThemeFacade"
      }
    }
  }
}
